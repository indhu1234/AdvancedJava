<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.grantuniversity.model.StudentCourse" table="STUDENTCOURSE">
		<id name="registrationNo" type="integer">
			<column name="REGISTRATIONNO" length="20" />
			<generator class="native" />
		</id>
		<property name="courseId" type="integer">
			<column name="COURSEID" length="20" />
		</property>
	</class>
</hibernate-mapping>